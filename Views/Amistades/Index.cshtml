@model IEnumerable<RedSocial.Models.Amistad>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Aceptada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailReciverNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailSenderNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Aceptada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailReciverNavigation.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailSenderNavigation.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
